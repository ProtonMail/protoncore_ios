    "ProtonCore-UIFoundations-V5": [
      "3.23.0"
    ]
  },
  "default_subspecs": [
    "ErrorWarningEmittingDefaultSubspec"
  ],
  "subspecs": [
    {
      "name": "ErrorWarningEmittingDefaultSubspec",
      "source_files": "libraries/ErrorWarningEmittingDefaultSubspec/ErrorWarningEmittingDefaultSubspec.swift"
    },
    {
      "name": "UsingCrypto",
      "dependencies": {
        "ProtonCore-Payments/UsingCrypto": [
          "3.23.0"
        ]
      },
      "source_files": [
        "libraries/PaymentsUI/Sources/Extensions/**/*.swift",
        "libraries/PaymentsUI/Sources/Managers/*.swift",
        "libraries/PaymentsUI/Sources/Views/*.swift",
        "libraries/PaymentsUI/Sources/PaymentsUI.swift",
        "libraries/PaymentsUI/Sources/Coordinators/*.swift",
        "libraries/PaymentsUI/Sources/V5/**/*.swift"
      ],
      "resource_bundles": {
        "Resources-PaymentsUI": [
          "libraries/PaymentsUI/Sources/Views/*.xib",
          "libraries/PaymentsUI/Sources/V5/**/*.xib",
          "libraries/PaymentsUI/Sources/PaymentsUI-V5.storyboard"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "swift-snapshot-testing": [

            ],
            "ProtonCore-TestingToolkit/UnitTests/Payments/UsingCrypto": [
              "3.23.0"
            ]
          },
          "source_files": "libraries/PaymentsUI/Tests/**/*.swift",
          "exclude_files": "libraries/PaymentsUI/Tests/PaymentsUIViewModelTests.swift"
        }
      ]
    },
    {
      "name": "UsingCryptoVPN",
      "dependencies": {
        "ProtonCore-Payments/UsingCryptoVPN": [
          "3.23.0"
        ]
      },
      "source_files": [
        "libraries/PaymentsUI/Sources/Extensions/**/*.swift",
        "libraries/PaymentsUI/Sources/Managers/*.swift",
        "libraries/PaymentsUI/Sources/Views/*.swift",
        "libraries/PaymentsUI/Sources/PaymentsUI.swift",
        "libraries/PaymentsUI/Sources/Coordinators/*.swift",
        "libraries/PaymentsUI/Sources/V5/**/*.swift"
      ],
      "resource_bundles": {
        "Resources-PaymentsUI": [
          "libraries/PaymentsUI/Sources/Views/*.xib",
          "libraries/PaymentsUI/Sources/V5/**/*.xib",
          "libraries/PaymentsUI/Sources/PaymentsUI-V5.storyboard"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "swift-snapshot-testing": [

            ],
            "ProtonCore-TestingToolkit/UnitTests/Payments/UsingCryptoVPN": [
              "3.23.0"
            ]
          },
          "source_files": "libraries/PaymentsUI/Tests/**/*.swift",
          "exclude_files": "libraries/PaymentsUI/Tests/PaymentsUIViewModelTests.swift"
        }
      ]
    }
  ],
  "swift_version": "5.6"
