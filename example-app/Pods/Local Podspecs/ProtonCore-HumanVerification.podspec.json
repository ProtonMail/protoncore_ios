{
  "name": "ProtonCore-HumanVerification",
  "version": "3.3.2",
  "summary": "shared frameworks",
  "description": "ios shared frameworks for all client apps",
  "homepage": "https://github.com/ProtonMail",
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "zhj4478": "feng@pm.me"
  },
  "source": {
    "git": null,
    "tag": "3.3.2"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.13"
  },
  "swift_versions": [
    "5.4"
  ],
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "ios": {
    "frameworks": "UIKit"
  },
  "osx": {
    "frameworks": "AppKit"
  },
  "dependencies": {
    "ProtonCore-UIFoundations": [
      "3.3.2"
    ],
    "ProtonCore-CoreTranslation": [
      "3.3.2"
    ],
    "ProtonCore-Foundations": [
      "3.3.2"
    ],
    "ProtonCore-Utilities": [
      "3.3.2"
    ]
  },
  "default_subspecs": [
    "ErrorWarningEmittingDefaultSubspec"
  ],
  "subspecs": [
    {
      "name": "ErrorWarningEmittingDefaultSubspec",
      "source_files": "libraries/ErrorWarningEmittingDefaultSubspec/ErrorWarningEmittingDefaultSubspec.swift"
    },
    {
      "name": "Alamofire",
      "dependencies": {
        "ProtonCore-APIClient/Alamofire": [
          "3.3.2"
        ]
      },
      "source_files": "libraries/HumanVerification/Sources/**/*.{h,m,swift}",
      "resource_bundles": {
        "Resources-HumanVerification": [
          "libraries/HumanVerification/Sources/**/*.{xib,storyboard,xcassets,geojson}"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/HumanVerification/Alamofire": [
              "3.3.2"
            ]
          },
          "source_files": "libraries/HumanVerification/Tests/**/*"
        }
      ]
    },
    {
      "name": "AFNetworking",
      "dependencies": {
        "ProtonCore-APIClient/AFNetworking": [
          "3.3.2"
        ]
      },
      "source_files": "libraries/HumanVerification/Sources/**/*.{h,m,swift}",
      "resource_bundles": {
        "Resources-HumanVerification": [
          "libraries/HumanVerification/Sources/**/*.{xib,storyboard,xcassets,geojson}"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/HumanVerification/AFNetworking": [
              "3.3.2"
            ]
          },
          "source_files": "libraries/HumanVerification/Tests/**/*"
        }
      ]
    }
  ],
  "swift_version": "5.4"
}
