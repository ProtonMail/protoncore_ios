{
  "name": "ProtonCore-PaymentsUI",
  "version": "3.2.0",
  "summary": "shared frameworks",
  "description": "ios shared frameworks for all client apps",
  "homepage": "https://github.com/ProtonMail",
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "zhj4478": "feng@pm.me"
  },
  "source": {
    "git": null,
    "tag": "3.2.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": [
    "5.1"
  ],
  "dependencies": {
    "ProtonCore-Log": [
      "3.2.0"
    ],
    "ProtonCore-CoreTranslation": [
      "3.2.0"
    ],
    "ProtonCore-Foundations": [
      "3.2.0"
    ],
    "ProtonCore-UIFoundations": [
      "3.2.0"
    ]
  },
  "default_subspecs": "UsingCrypto",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "subspecs": [
    {
      "name": "UsingCrypto",
      "dependencies": {
        "ProtonCore-Payments/UsingCrypto": [
          "3.2.0"
        ]
      },
      "source_files": "libraries/PaymentsUI/Sources/**/*.swift",
      "resource_bundles": {
        "Resources-PaymentsUI": [
          "libraries/PaymentsUI/Sources/Assets.xcassets",
          "libraries/PaymentsUI/Sources/**/*.xib",
          "libraries/PaymentsUI/Sources/**/*.storyboard"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/Payments/UsingCrypto": [
              "3.2.0"
            ]
          },
          "source_files": "libraries/PaymentsUI/Tests/**/*.swift"
        }
      ]
    },
    {
      "name": "UsingCryptoVPN",
      "dependencies": {
        "ProtonCore-Payments/UsingCryptoVPN": [
          "3.2.0"
        ]
      },
      "source_files": "libraries/PaymentsUI/Sources/**/*.swift",
      "resource_bundles": {
        "Resources-PaymentsUI": [
          "libraries/PaymentsUI/Sources/Assets.xcassets",
          "libraries/PaymentsUI/Sources/**/*.xib",
          "libraries/PaymentsUI/Sources/**/*.storyboard"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/Payments/UsingCryptoVPN": [
              "3.2.0"
            ]
          },
          "source_files": "libraries/PaymentsUI/Tests/**/*.swift"
        }
      ]
    }
  ],
  "swift_version": "5.1"
}
