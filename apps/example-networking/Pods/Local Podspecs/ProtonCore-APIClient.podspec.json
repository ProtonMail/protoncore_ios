  "source_files": "libraries/Networking/Sources/APIClient/**/*.swift",
  "prepare_command": "bash libraries/Networking/Tests/APIClient/Scripts/prepare_obfuscated_constants.sh",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "preserve_paths": "libraries/Networking/Tests/APIClient/Scripts/*",
      "script_phases": {
        "name": "Obfuscation",
        "script": "${PODS_TARGET_SRCROOT}/libraries/Networking/Tests/APIClient/Scripts/prepare_obfuscated_constants.sh",
        "execution_position": "before_compile",
        "output_files": [
          "${PODS_TARGET_SRCROOT}/libraries/Networking/Tests/APIClient/ObfuscatedConstants.swift"
        ]
      },
      "source_files": [
        "libraries/Networking/Tests/APIClient/*.swift",
        "libraries/Networking/Tests/APIClient/Mocks/*.swift"
      ],
      "dependencies": {
        "OHHTTPStubs/Swift": [

        ],
        "TrustKit": [

        ]
      }
    }
