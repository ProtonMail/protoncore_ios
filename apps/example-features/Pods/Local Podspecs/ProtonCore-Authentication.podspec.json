{
  "name": "ProtonCore-Authentication",
  "version": "2.6.0",
  "summary": "shared frameworks",
  "description": "ios shared frameworks for all client apps",
  "homepage": "https://github.com/ProtonMail",
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "zhj4478": "feng@pm.me"
  },
  "source": {
    "git": null,
    "tag": "2.6.0"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.12.1"
  },
  "swift_versions": [
    "5.1"
  ],
  "dependencies": {
    "ProtonCore-Services": [
      "2.6.0"
    ],
    "ProtonCore-APIClient": [
      "2.6.0"
    ]
  },
  "prepare_command": "bash libraries/Authentication/Scripts/prepare_obfuscated_constants.sh",
  "default_subspecs": "UsingCrypto",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "subspecs": [
    {
      "name": "UsingCrypto",
      "dependencies": {
        "ProtonCore-Crypto": [
          "2.6.0"
        ]
      },
      "source_files": [
        "libraries/Authentication/Sources/*.swift",
        "libraries/Authentication/Sources/**/*.swift"
      ],
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "preserve_paths": "libraries/Authentication/Scripts/*",
          "script_phases": {
            "name": "Obfuscation",
            "script": "${PODS_TARGET_SRCROOT}/libraries/Authentication/Scripts/prepare_obfuscated_constants.sh",
            "execution_position": "before_compile",
            "output_files": [
              "${PODS_TARGET_SRCROOT}/libraries/Authentication/Tests/TestData/ObfuscatedConstants.swift"
            ]
          },
          "source_files": [
            "libraries/Authentication/Tests/TestData/ObfuscatedConstants.swift",
            "libraries/Authentication/Tests/**/*.swift"
          ],
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/Authentication/UsingCrypto": [
              "2.6.0"
            ],
            "OHHTTPStubs/Swift": [

            ]
          }
        }
      ]
    },
    {
      "name": "UsingCryptoVPN",
      "dependencies": {
        "ProtonCore-Crypto-VPN": [
          "2.6.0"
        ]
      },
      "source_files": [
        "libraries/Authentication/Sources/*.swift",
        "libraries/Authentication/Sources/**/*.swift"
      ],
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "preserve_paths": "libraries/Authentication/Scripts/*",
          "script_phases": {
            "name": "Obfuscation",
            "script": "${PODS_TARGET_SRCROOT}/libraries/Authentication/Scripts/prepare_obfuscated_constants.sh",
            "execution_position": "before_compile",
            "output_files": [
              "${PODS_TARGET_SRCROOT}/libraries/Authentication/Tests/TestData/ObfuscatedConstants.swift"
            ]
          },
          "source_files": [
            "libraries/Authentication/Tests/TestData/ObfuscatedConstants.swift",
            "libraries/Authentication/Tests/**/*.swift"
          ],
          "dependencies": {
            "ProtonCore-TestingToolkit/UnitTests/Authentication/UsingCryptoVPN": [
              "2.6.0"
            ],
            "OHHTTPStubs/Swift": [

            ]
          }
        }
      ]
    }
  ],
  "swift_version": "5.1"
}
