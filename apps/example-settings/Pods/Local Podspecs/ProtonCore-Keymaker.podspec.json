  "subspecs": [
    {
      "name": "UsingCrypto",
      "dependencies": {
        "ProtonCore-Crypto": [
          "2.7.0"
        ]
      },
      "source_files": [
        "libraries/Keymaker/Sources/*.swift",
        "libraries/Keymaker/Sources/**/*.swift"
      ],
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "source_files": "libraries/Keymaker/Tests/**/*"
        }
      ]
    },
    {
      "name": "UsingCryptoVPN",
      "dependencies": {
        "ProtonCore-Crypto-VPN": [
          "2.7.0"
        ]
      },
      "source_files": [
        "libraries/Keymaker/Sources/*.swift",
        "libraries/Keymaker/Sources/**/*.swift"
      ],
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "source_files": "libraries/Keymaker/Tests/**/*"
        }
      ]
    }
  ],
  "swift_version": "5.1"
