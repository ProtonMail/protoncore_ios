{
    "go_version":"1.15.15",
    "build_dir":"build",
    "out_dir":"out",
    "go_mobile_dir":"mobile",
    "go_mobile_flags": [
        "-x",
        "-ldflags=\"-s -w\""
    ],
    "build_name":"Crypto_VPN",
    "commit_message": "Build for gopenpgp v2.3.1 and go-srp v0.0.3",
    "upload_branch": "feature/crypto-vpn-update-2.3.1+0.0.3",
    "java_pkg":"com.proton.Crypto_VPN",
    "targets":["apple"],
    "requirements": [
        {
            "module":
            {
                "path":"github.com/ProtonMail/gopenpgp/v2",
                "version":"v2.3.1"
            },
            "packages": ["crypto", "armor", "constants", "models", "subtle", "helper"]
        },
        {
            "module":{
                "path":"github.com/ProtonMail/go-srp",
                "version": "v0.0.3"
            }
        },
        {
            "module": {
                "path":"gitlab.protontech.ch/ProtonVPN/development/clients-shared.git",
                "version": "v0.0.0"
            },
            "packages": ["ed25519", "localAgent", "vpnPing"]
        }
    ],
    "replacements":[
        {
            "old": {
                "path":"gitlab.protontech.ch/ProtonVPN/development/clients-shared.git"
            },
            "new": {
                "path": "github.com/ProtonVPN/go-vpn-lib",
                "version": "bac6105c7c75699d06aa4e375fc0874a721588ed"
            }
        }
    ]
}