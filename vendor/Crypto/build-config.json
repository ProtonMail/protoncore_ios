{
    "go_version":"1.18.3",
    "build_dir":"build",
    "out_dir":"out",
    "go_mobile_dir":"mobile",
    "go_mobile_flags": [
        "-x",
        "-ldflags=\"-s -w\""
    ],
    "build_name":"Crypto",
    "commit_message": "Test build for gopenpgp v2.4.8 and go-srp v0.0.5, using fork of golang 1.18.3",
    "upload_branch": "feature/crypto-update-2.4.8+0.0.5-test-build",
    "java_pkg":"com.proton.Crypto",
    "targets":["apple"],
    "requirements": [
        {
            "module":
            {
                "path":"github.com/ProtonMail/gopenpgp/v2",
