{
    "go_version":"1.17.9",
    "build_dir":"build",
    "out_dir":"out",
    "go_mobile_dir":"mobile",
    "go_mobile_flags": [
        "-x",
        "-ldflags=\"-s -w\""
    ],
    "build_name":"GoLibs",
    "commit_message": "Crypto build, targeting iOS >=11",
    "upload_branch": "fix/crypto-update",
    "destination_folder": "vendor/Crypto",
    "targets":["apple"],
    "min_ios_version": "11.0",
    "requirements": [
        {
            "module":
            {
                "path":"github.com/ProtonMail/gopenpgp/v2",
                "version":"v2.4.10"
            },
            "packages": ["crypto", "armor", "constants", "models", "subtle", "helper"]
        },
        {
            "module":{
                "path":"github.com/ProtonMail/go-srp",
                "version": "v0.0.5"
            }
        }
    ]
}
